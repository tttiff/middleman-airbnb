<div>
  <ul class="list-inline team">
    <h1>This template was built with <i class="fas fa-heart"></i> by</h1>
  <% data.users.each do |user| %>
    <li>
      <% link_to "https://kitt.lewagon.com/alumni/#{user['github']}", target: "_blank" do %>
        <div>
          <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user['github']}", class: "img-circle team" %>
          <h3><%= user.name.capitalize %></h3>
          <h4><%= user.github.capitalize %></h4>
        </div>
      <% end %>
    </li>
  <% end %>
  </ul>
</div>
